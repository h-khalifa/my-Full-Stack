@model ExO.Models.StudentExamStatusViewModel


<div style="margin:10%;">
    <div class="panel panel-success">
        <div class="panel-heading">
            <h3 class="panel-title">@Model.ExamName <span class="glyphicon glyphicon-pencil"></span>  </h3>
        </div>
        <div class="panel-body">
            <p>
                Your Exam is <b class="text-success">Finished</b>.
                <div class="well">
                    your score is: <span class="text-primary">@Model.Score <span>/</span> @Model.ExamSize </span>
                </div>
            </p>
        </div>
    </div>
</div>